{
  "expo": {
    "name": "ePRX UV2",
    "slug": "eprx-uv2-monorepo",
    "scheme": "eprx-uv2",
    "version": "1.0.0",
    "owner": "ramdac17",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "dark",
    "jsEngine": "hermes",
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "android": {
      "package": "com.eprx.uv2",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#000000"
      }
    },
    "ios": {
      "bundleIdentifier": "com.eprx.uv2",
      "supportsTablet": true
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "3126d6a3-ecd6-4fe8-a046-da6ead5eb3df"
      }
    }
  }
}