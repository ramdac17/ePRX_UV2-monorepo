{
  "name": "eprx_uv1-monorepo",
  "private": true,
  "packageManager": "pnpm@9.15.4",
  "engines": {
    "node": ">=22.12.0"
  },
  "scripts": {
    "db:studio": "prisma studio --url $(grep DATABASE_URL apps/api/.env | cut -d '=' -f2 | sed 's/\"//g')",
    "dev": "turbo run dev",
    "build": "turbo run build",
    "build:api": "turbo run build --filter=api",
    "sync-ip": "echo 'Script missing'",
    "postinstall": "prisma generate --schema=apps/api/prisma/schema.prisma",
    "start:railway": "npx prisma migrate deploy && node dist/apps/api/main.js",
    "android": "turbo run android",
    "ios": "turbo run ios"
  },
  "devDependencies": {
    "prettier": "^3.7.4",
    "turbo": "^2.8.1",
    "typescript": "5.9.2"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "react",
        "react-dom",
        "@types/react"
      ]
    },
    "overrides": {
      "react": "19.1.0",
      "react-native": "0.81.5"
    }
  },
  "dependencies": {
    "@prisma/client": "^7.3.0",
    "cross-env": "^7.0.3",
    "prisma": "^7.3.0"
  },
  "prisma": {
    "schema": "apps/api/prisma/schema.prisma"
  },
  "version": "1.0.0"
}