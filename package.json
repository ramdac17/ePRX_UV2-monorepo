{
  "name": "eprx_uv1-monorepo",
  "private": true,
  "packageManager": "pnpm@9.15.4",
  "engines": {
    "node": ">=22.12.0"
  },
  "scripts": {
    "db:studio": "prisma studio --url $(grep DATABASE_URL apps/api/.env | cut -d '=' -f2 | sed 's/\"//g')",
    "dev": "node scripts/update-ip.js && turbo run dev",
    "build": "turbo run build",
    "build:api": "turbo run build --filter=api",
    "sync-ip": "node scripts/update-ip.js",
    "start:api": "node dist/apps/api/main.js",
    "postinstall": "prisma generate --schema=apps/api/prisma/schema.prisma",
    "start:railway": "npx prisma migrate deploy && node dist/apps/api/main.js"
  },
  "devDependencies": {
    "prettier": "^3.7.4",
    "turbo": "^2.8.1",
    "typescript": "5.9.2"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "react",
        "react-dom",
        "@types/react"
      ]
    },
    "overrides": {
      "react": "19.1.0",
      "react-dom": "19.1.0",
      "react-native": "0.81.5",
      "react-native-svg": "15.12.1"
    },
    "neverBuiltDependencies": [
      "sharp"
    ]
  },
  "dependencies": {
    "@prisma/client": "^7.3.0",
    "cross-env": "^7.0.3",
    "prisma": "^7.3.0"
  },
  "prisma": {
    "schema": "apps/api/prisma/schema.prisma"
  }
}